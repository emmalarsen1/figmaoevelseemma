---
import MainLayout from "../layouts/MainLayout.astro";
import TitlePillPage from "../components/general/TitlePillPage.astro";
import Banner from "../components/general/Banner.astro";
import "../styles/global.css";
import "../styles/reset.css";
import PillTitleText from "../components/general/PillTitleText.astro";
import OurVisionPart from "../components/about/OurVisionPart.astro";
import GrowingWithPart from "../components/about/GrowingWithPart.astro";
import VideoPart from "../components/about/VideoPart.astro";
import OurTeam from "../components/about/OurTeam.astro";
const { iconname } = Astro.props;
---

<MainLayout>
  <Banner
    imgPath="/src/assets/images/aboutbanner.jpg"
    name="about banner"
    title="about"
    subtitle="Creative Services For Boost Your Business Growth"
  />
  <OurVisionPart
    imgPath="/src/assets/images/about2.jpg"
    name="about foto nr 2"
  />

  <GrowingWithPart />
  <VideoPart
    imgPath="/src/assets/images/videofoto.jpg"
    name="videofoto"
    iconname="play-button"
  />
  <OurTeam />
</MainLayout>

